    "eslint-config-airbnb-typescript": "^17.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "yarn lint:fix",
      "git add"
    ]
